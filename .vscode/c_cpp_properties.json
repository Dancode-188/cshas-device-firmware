        "${workspaceFolder}/**",
        "C:/Users/User/.platformio/packages/framework-arduinoespressif8266/tools/sdk/include",
        "C:/Users/User/.platformio/packages/framework-arduinoespressif8266/tools/sdk/lwip2/include",
        "C:/Users/User/.platformio/packages/framework-arduinoespressif8266/cores/esp8266",
        "C:/Users/User/.platformio/packages/framework-arduinoespressif8266/variants/nodemcu",
        "C:/Users/User/.platformio/packages/framework-arduinoespressif8266/libraries/ESP8266WiFi/src"
